    },
    "gp_section_571652763964932935": {
      "type": "gp-section-571652763964932935",
      "disabled": true,
      "settings": {
        "section_preload": "false",
        "checksum": "ade336d0f9850c6aeb4d2cd607e361b4c173c41fb7966053f1e387bd02843545_1_1_1_0_",
        "ggmjkqqGpr6_text": "Cum se montează ?",
        "ggZH7x9wl6y_text": "<p>Aquafiltr se montează între capul de duș și perete, ceea ce îl face compatibil cu orice baie și orice tip de duș.</p>",
        "ggeYPtX_j-v_text": "Am nevoie de instalator?",
        "ggXoePKD7Fr_text": "<p><strong>Nu. </strong>Aquafiltr<strong> </strong>se instalează ușor, în doar câțiva pași simpli, fără unelte speciale.<br><strong>Pur și simplu îl înșurubezi și e gata.</strong></p>"
      }
